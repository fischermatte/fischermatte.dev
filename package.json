    "react-dom": "18.3.1",
    "rxjs": "^7.8.1",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
